{
  "name": "css_utilities",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/plake492/css_utilities.git",
  "author": "<plake492@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && sass ./src/index.scss:./dist/index.css --style compressed && sass ./src/reset.scss:./dist/reset.css --style compressed",
    "watch": "sass -w ./src/index.scss:./dev/src/styles/index.css --style compressed && sass ./src/reset.scss:./dev/src/styles/reset.css"
  },
  "devDependencies": {
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0"
  }
}
